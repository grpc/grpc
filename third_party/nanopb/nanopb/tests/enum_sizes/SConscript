# Test that different sizes of enum fields are properly encoded and decoded.

Import('env')

env.NanopbProto('enumsizes')

p = env.Program(["enumsizes_unittests.c",
                 "enumsizes.pb.c",
                 "$COMMON/pb_encode.o",
                 "$COMMON/pb_decode.o",
                 "$COMMON/pb_common.o"])
env.RunTest(p)
