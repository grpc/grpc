        ":gevent",
        ":session_cache",
    ],
)

py_library(
    name = "gevent",
    srcs = ["gevent.py"],
    deps = [
        "//src/python/grpcio/grpc/_cython:cygrpc",
    ],
)

py_library(
    name = "session_cache",
    srcs = ["session_cache.py"],
    deps = [
        "//src/python/grpcio/grpc/_cython:cygrpc",
    ],
