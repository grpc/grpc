{
  "name": "grpc/grpc-dev",
  "description": "gRPC library for PHP - for Development use only",
  "license": "Apache-2.0",
  "version": "1.79.0",
  "require": {
    "php": ">=7.1.0",
    "ext-grpc": "*",
    "ext-json": "*",
    "google/protobuf": "^v3.3.0"
  },
  "require-dev": {
    "google/auth": "^v1.3.0",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Grpc\\": "lib/Grpc/",
      "": ["tests/interop/",
           "tests/generated_code/"]
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse -c phpstan.neon"
  }
}
