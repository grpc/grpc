<Project Sdk="Microsoft.NET.Sdk">

  <!--
     Note: common properties and targets for tests are imported from the
     Directory.Build.props and Directory.Build.targets files in the parent directoty
  -->

  <PropertyGroup>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <!-- for these tests assume protoc supports the enable_nrt option -->
    <Protobuf_EnableNRT_Supported>true</Protobuf_EnableNRT_Supported>

    <!-- force nullable off in generated code -->
    <gRPC_NullableReferenceTypes>disable</gRPC_NullableReferenceTypes>
    <Protobuf_NullableReferenceTypes>disable</Protobuf_NullableReferenceTypes>
  </PropertyGroup>

  <ItemGroup>
    <Protobuf Include="file.proto" GrpcServices="server" />
  </ItemGroup>
  
</Project>
