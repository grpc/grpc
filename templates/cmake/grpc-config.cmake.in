# Depend packages
if(NOT ZLIB_FOUND)
  find_package(ZLIB)
endif()
if(NOT OPENSSL_FOUND)
  find_package(OpenSSL)
endif()

# Current dir
get_filename_component(_GRPC_PACKAGE_PREFIX
  "${CMAKE_CURRENT_LIST_FILE}" PATH)

# Imported targets
include("${_GRPC_PACKAGE_PREFIX}/grpc-targets.cmake")

# Compute the installation prefix relative to this file.
get_filename_component(_GRPC_IMPORT_PREFIX
  "${_GRPC_PACKAGE_PREFIX}" PATH)
get_filename_component(_GRPC_IMPORT_PREFIX
  "${_GRPC_IMPORT_PREFIX}" PATH)
get_filename_component(_GRPC_IMPORT_PREFIX
  "${_GRPC_IMPORT_PREFIX}" PATH)

# Cleanup temporary variables.
set(_GRPC_PACKAGE_PREFIX)
set(_GRPC_IMPORT_PREFIX)
